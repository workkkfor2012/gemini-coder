{
  "name": "gemini-coder",
  "displayName": "Code Web Chat (CWC, prev. Gemini Coder)",
  "description": "Initialize any web chat with your code",
  "version": "1.30.0",
  "scripts": {
    "build": "npx vsce package --no-dependencies",
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --config ./webpack.config.js --mode production",
    "watch": "webpack --config ./webpack.config.js --watch --mode development",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "test": "jest"
  },
  "publisher": "robertpiosik",
  "homepage": "https://codeweb.chat/",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertpiosik/CodeWebChat"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "sponsor": {
    "url": "https://buymeacoffee.com/robertpiosik"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "AI",
    "Chat"
  ],
  "keywords": [
    "ai",
    "autocomplete",
    "refactoring",
    "assistant",
    "fim",
    "chat",
    "free",
    "AI Studio",
    "Gemini",
    "ChatGPT",
    "Claude",
    "DeepSeek",
    "Qwen",
    "Grok",
    "Mistral",
    "HuggingChat",
    "openai",
    "typescript",
    "javascript",
    "go",
    "golang",
    "json",
    "php",
    "ruby",
    "kotlin",
    "tools",
    "c++",
    "c#"
  ],
  "main": "out/extension.js",
  "activationEvents": [],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "icon": "media/logo.png",
